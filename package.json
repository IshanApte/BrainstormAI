{
  "name": "brainstormai",
  "version": "1.0.0",
  "description": "AI-powered brainstorming assistant with automatic note-taking",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm install && npm run build",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "ai",
    "brainstorming",
    "notes",
    "langchain",
    "react"
  ],
  "author": "BrainstormAI Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@langchain/langgraph": "^0.2.74",
    "jspdf": "^3.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^7.5.0",
    "langchain": "^0.0.147",
    "openai": "^4.11.1",
    "@langchain/openai": "^0.0.14",
    "uuid": "^9.0.0",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0"
  }
}
